{
  "name": "backend-rust",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "generators": {},
  "sourceRoot": "apps/backend-rust/src",
  "targets": {
    "serve": {
      "executor": "nx:run-commands",
      "options": {
        "command": "cargo run",
        "cwd": "apps/backend-rust"
      }
    },
    "build": {
      "executor": "nx:run-commands",
      "options": {
        "command": "cargo build --release",
        "cwd": "apps/backend-rust"
      }
    }
  }
}
